<program_settings>
	<filenames>
		<forward_sequences>@HWI-M02942_file1</forward_sequences>
		<reverse_sequences>@HWI-M02942_file2</reverse_sequences>
		<sequence_data>champignon_data.bin</sequence_data>
		<log_file>log.txt</log_file>
	</filenames>
	<default_cutoffs>
		<e-value>0.01</e-value>
	</default_cutoffs>
</program_settings>

<parameter_settings>
	<blastn>
		<default_parameters>
			<database>nr</database>
			<algoritme>blastn</algoritme>
			<expect_threshold>10</expect_threshold>
			<low_complexity>True</low_complexity>
			<mask_lookup_only>True</mask_lookup_only>
			<match_mismatch_score>1,-2</match_mismatch_score>
			<gap_costs>linear</gap_costs>
		</default_parameters>
	</blastn>
	<blastx>
		<default_parameters>
			<database>nr</database>
			<matrix>BLOSUM62</matrix>
			<low_complexity>True</low_complexity>
			<mask_lookup_only>False</mask_lookup_only>
			<gap_costs>existence:11,extension:1</gap_costs>
		</default_parameters>
	</blastx>
	<tblastx>
		<default_parameters>
			<database>nr</database>
			<matrix>BLOSUM62</matrix>
			<low_complexity>True</low_complexity>
			<mask_lookup_only>False</mask_lookup_only>
		</default_parameters>
	</tblastx>
</parameter_settings>
